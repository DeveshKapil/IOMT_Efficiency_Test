# Firebase Functions Environment Configuration
# Copy this file to .env and update the values

# Your receiver endpoint (update this with your actual receiver URL)
RECEIVER_URL=http://localhost:5000

# For production, use your public receiver URL
# RECEIVER_URL=https://your-receiver-domain.com

# Logging level
LOG_LEVEL=info

# Request timeout (in milliseconds)
REQUEST_TIMEOUT=30000

# CORS origins (comma-separated)
CORS_ORIGINS=http://localhost:3000,http://localhost:5000 